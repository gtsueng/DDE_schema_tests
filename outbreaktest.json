{
  "@context": {
    "schema": "http://schema.org/",
    "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "outbreaktest": "http://discovery.biothings.io/view/"
  },
  "@graph": [
    {
      "@id": "outbreaktest:AuthorRole",
      "@type": "rdfs:Class",
      "rdfs:comment": "The role the author played in the creation of a resource",
      "rdfs:label": "AuthorRole",
      "rdfs:subClassOf": {
        "@id": "schema:Role"
      },
      "$validation": {
        "$schema": "http://json-schema.org/draft-07/schema#",
        "type": "object",
        "properties": {
          "relatedWork": {
            "description": "The resource or creative work in which the author played this role",
            "@type": "CreativeWork",
            "type": "object",
            "properties": {
              "name": {
                "type": "string"
              },
              "url": {
                "type": "string",
                "format": "uri"
              },
              "identifier": {
                "type": "string"
              }
            },
            "required": [
              "name"
            ]
          },
          "roleName": {
            "description": "A role played, performed or filled by a person or organization. For example, the team of creators for a comic book might fill the roles named 'inker', 'penciller', and 'letterer'; or an athlete in a SportsTeam might play in the position named 'Quarterback'.",
            "type": "string"
          }
        },
        "required": [
        ]
      }
    },
    {
      "@id": "outbreaktest:relatedWork",
      "@type": "rdfs:Property",
      "rdfs:comment": "The resource or creative work in which the author played this role",
      "rdfs:label": "relatedWork",
      "schema:domainIncludes": {
        "@id": "outbreaktest:AuthorRole"
      },
      "schema:rangeIncludes": [
        {
          "@id": "schema:CreativeWork"
        }
      ]
    },
    {
      "@id": "outbreaktest:ObPerson",
      "@type": "rdfs:Class",
      "rdfs:comment": "This is the Person schema for describing authors and people for outbreak.info resources",
      "rdfs:label": "ObPerson",
      "rdfs:subClassOf": {
        "@id": "schema:Person"
      },
      "$validation": {
        "$schema": "http://json-schema.org/draft-07/schema#",
        "type": "object",
        "properties": {
          
          "familyName": {
            "description": "Family name. In the U.S., the last name of an Person. This can be used along with givenName instead of the name property.",
            "type": "string"
          },
          "givenName": {
            "description": "Given name. In the U.S., the first name of a Person. This can be used along with familyName instead of the name property.",
            "type": "string"
          },
          "affiliation": {
            "description": "An organization that this person is affiliated with. For example, a school/university, a club, or a team.",
            "oneOf": [
              {
                "@type": "Organization",
                "type": "object",
                "properties": {
                  "name": {
                    "type": "string"
                  },
                  "altnernateName": {
                    "type": "string"
                  }
                },
                "required": [
                  "name"
                ]
              },
              {
                "type": "array",
                "items": {
                    "@type": "Organization",
                    "type": "object",
                    "properties": {
                      "name": {
                        "type": "string"
                      },
                      "altnernateName": {
                        "type": "string"
                      }
                    },
                    "required": [
                      "name"
                    ]
                }
              }
            ]
          },
          "alternateName": {
            "description": "An alias for the item.",
            "type": "string"
          },
          "name": {
            "description": "The name of the item.",
            "type": "string"
          },
          "orcid": {
            "description": "the ORCID ID of the person",
            "type": "string"
          },
          "role": {
            "description": "authorship or contribution roles played in related resources",
            "oneOf": [
              {
                "$ref": "#/definitions/authorrole"
              },
              {
                "type": "array",
                "items": {
                    "$ref": "#/definitions/authorrole"
                }
              }
            ]
            }
          }
        },
        "required": [
            "name"
        ],
        "definitions": {
            "authorrole":{
                "type": "object",
                "@type": "outbreaktest:AuthorRole",
                "properties": {
                  "relatedWork": {
                    "description": "The resource or creative work in which the author played this role",
                    "@type": "CreativeWork",
                    "type": "object",
                    "properties": {
                      "name": {
                        "type": "string"
                      },
                      "url": {
                        "type": "string",
                        "format": "uri"
                      },
                      "identifier": {
                        "type": "string"
                      }
                    },
                    "required": [
                      "name"
                    ]
                  },
                  "roleName": {
                    "description": "A role played, performed or filled by a person or organization. For example, the team of creators for a comic book might fill the roles named 'inker', 'penciller', and 'letterer'; or an athlete in a SportsTeam might play in the position named 'Quarterback'.",
                    "type": "string"
                  }
                },
                "required": [
                ]
            }
        }
    },
    {
      "@id": "outbreaktest:orcid",
      "@type": "rdfs:Property",
      "rdfs:comment": "the ORCID ID of the person",
      "rdfs:label": "orcid",
      "schema:domainIncludes": {
        "@id": "outbreaktest:ObPerson"
      },
      "schema:rangeIncludes": [
        {
          "@id": "schema:Text"
        }
      ]
    },
    {
      "@id": "outbreaktest:role",
      "@type": "rdfs:Property",
      "rdfs:comment": "authorship or contribution roles played in related resources",
      "rdfs:label": "authorshipRoles",
      "schema:domainIncludes": {
        "@id": "outbreaktest:ObPerson"
      },
      "schema:rangeIncludes": [
        {
          "@id": "outbreaktest:AuthorRole"
        }
      ]
    }
  ]
}