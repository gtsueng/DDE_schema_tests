                        "type": "string"
                    },
                    "alternateName": {
                        "type": "string"
                    },
                    "givenName": {
                        "type": "string"
                    },
                    "familyName": {
                        "type": "string"
                    },
                    "orcid": {
                        "type": "string"
                    },
                    "affiliation": {
                        "description": "consortia or other organizations with which this person is affiliated",
                        "oneOf": [
                          {
                            "$ref": "#/definitions/baseOrgObject"
                          },
                          {
                            "type": "array",
                            "items": {
