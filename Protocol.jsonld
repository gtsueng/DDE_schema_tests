{
  "@context": {
    "schema": "http://schema.org/",
    "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "outbreaktest": "http://discovery.biothings.io/view/"
  },
  "@graph": [
    {
      "@id": "outbreaktest:Protocol",
      "@type": "rdfs:Class",
      "rdfs:comment": "This is the schema for describing the Protocols for outbreak.info Resources.",
      "rdfs:label": "Protocol",
      "rdfs:subClassOf": {
        "@id": "schema:HowTo"
      },
      "$validation": {
        "$schema": "http://json-schema.org/draft-07/schema#",
        "type": "object",
        "properties": {
          "doi": {
            "description": "the DOI of the protocol",
            "type": "string"
          },
          "warning": {
            "description": "Precautions, warnings, and/or safety warnings associated with or highlighted by the protocol",
            "type": "string"
          },
          "correctionNote": {
            "description": "statement of what was updated/changed since prior version",
            "type": "string"
          },
          "protocolStatus": {
            "description": "The status of the protocol",
            "oneOf": [
              {
                "type": "string",
                "enum": [
                  "Working--In use by author",
                  "Working--not in use by author",
                  "Working--in use by others",
                  "Not working",
                  "In Development",
                  "Deprecated"
                ]
              },
              {
                "type": "array",
                "items": {
                  "type": "string",
                  "enum": [
                    "Working--In use by author",
                    "Working--not in use by author",
                    "Working--in use by others",
                    "Not working",
                    "In Development",
                    "Deprecated"
                  ]
                }
              }
            ]
          },
          "instrument": {
            "description": "A laboratory equipment use by a person to follow one or more steps described in this Protocol",
            "@type": "outbreaktest:Instrument",
            "type": "object",
            "properties": {
              "name": {
                "type": "string"
              },
              "url": {
                "type": "string",
                "format": "uri"
              }
            },
            "required": [
              "name"
            ]
          },
          "measurementTechnique": {
            "description": "A technique or technology used in this protocol",
            "@type": "CreativeWork",
            "type": "string",
            "vocabulary": {
              "ontology": [
                "[For example:]",
                "ncbitaxon"
              ],
              "children_of": [
                "[For example:]",
                "http://purl.obolibrary.org/obo/NCBITaxon_10239"
              ]
            },
            "strict": false
          },
          "protocolSetting": {
            "description": "The type of 'protocol' based on setting in which it would apply",
            "oneOf": [
              {
                "type": "string",
                "enum": [
                  "clinical",
                  "experimental",
                  "field",
                  "computational",
                  "public"
                ]
              },
              {
                "type": "array",
                "items": {
                  "type": "string",
                  "enum": [
                    "clinical",
                    "experimental",
                    "field",
                    "computational",
                    "public"
                  ]
                }
              }
            ]
          },
          "protocolCategory": {
            "description": "The type of 'protocol' based on degree of specificity and purpose",
            "oneOf": [
              {
                "type": "string",
                "enum": [
                  "protocol",
                  "procedure",
                  "policy",
                  "guideline"
                ]
              },
              {
                "type": "array",
                "items": {
                  "type": "string",
                  "enum": [
                    "protocol",
                    "procedure",
                    "policy",
                    "guideline"
                  ]
                }
              }
            ]
          },
          "usedToGenerate": {
            "description": "Type of evidence/data generated by the protocol",
            "@type": "CreativeWork",
            "type": "string",
            "vocabulary": {
              "ontology": [
                "[For example:]",
                "ncbitaxon"
              ],
              "children_of": [
                "[For example:]",
                "http://purl.obolibrary.org/obo/NCBITaxon_10239"
              ]
            },
            "strict": false
          },
          "inComplianceWith": {
            "description": "Guidelines or Standards to which these protocols adhere. ie- GMP, GCP, other EQUATOR guidelines. Useful for selecting protocols where certain standards/guidelines must be met",
            "oneOf": [
              {
                "type": "string"
              },
              {
                "type": "array",
                "items": {
                  "type": "string"
                }
              }
            ]
          },
          "description": {
            "description": "A description of the item.",
            "oneOf": [
              {
                "type": "string"
              },
              {
                "type": "array",
                "items": {
                  "type": "string"
                }
              }
            ]
          },
          "identifier": {
            "description": "The identifier property represents any kind of identifier for any kind of <a class=\"localLink\" href=\"http://schema.org/Thing\">Thing</a>, such as ISBNs, GTIN codes, UUIDs etc. Schema.org provides dedicated properties for representing many of these, either as textual strings or as URL (URI) links. See <a href=\"/docs/datamodel.html#identifierBg\">background notes</a> for more details.",
            "oneOf": [
              {
                "type": "string"
              },
              {
                "type": "array",
                "items": {
                  "type": "string"
                }
              }
            ]
          },
          "name": {
            "description": "The name of the item.",
            "oneOf": [
              {
                "type": "string"
              },
              {
                "type": "array",
                "items": {
                  "type": "string"
                }
              }
            ]
          },
          "url": {
            "description": "URL of the item.",
            "type": "string",
            "format": "uri"
          },
          "author": {
            "description": "The author of this content or rating. Please note that author is special in that HTML 5 provides a special mechanism for indicating authorship via the rel tag. That is equivalent to this and may be used interchangeably.",
            "@type": "outbreaktest:Person",
            "type": "object",
            "properties": {
              "name": {
                "type": "string"
              },
              "url": {
                "type": "string",
                "format": "uri"
              }
            },
            "required": [
              "name"
            ]
          },
          "dateModified": {
            "description": "The date on which the CreativeWork was most recently modified or when the item's entry was modified within a DataFeed.",
            "format": "date",
            "type": "string"
          },
          "datePublished": {
            "description": "Date of first broadcast/publication.",
            "format": "date",
            "type": "string"
          },
          "keywords": {
            "description": "Keywords or tags used to describe this content. Multiple entries in a keywords list are typically delimited by commas.",
            "oneOf": [
              {
                "type": "string"
              },
              {
                "type": "array",
                "items": {
                  "type": "string"
                }
              }
            ]
          },
          "license": {
            "description": "A license document that applies to this content, typically indicated by URL.",
            "type": "string"
          },
          "material": {
            "description": "A material that something is made from, e.g. leather, wool, cotton, paper.",
            "@type": "outbreaktest:Product",
            "type": "object",
            "properties": {
              "name": {
                "type": "string"
              },
              "url": {
                "type": "string",
                "format": "uri"
              }
            },
            "required": [
              "name"
            ]
          }
        },
        "required": [
          "description",
          "identifier",
          "name",
          "url",
          "author"
        ]
      }
    },
    {
      "@id": "outbreaktest:doi",
      "@type": "rdfs:Property",
      "rdfs:comment": "the DOI of the protocol",
      "rdfs:label": "doi",
      "schema:domainIncludes": {
        "@id": "outbreaktest:Protocol"
      },
      "schema:rangeIncludes": [
        {
          "@id": "schema:Text"
        }
      ]
    },
    {
      "@id": "outbreaktest:warning",
      "@type": "rdfs:Property",
      "rdfs:comment": "Precautions, warnings, and/or safety warnings associated with or highlighted by the protocol",
      "rdfs:label": "warning",
      "schema:domainIncludes": {
        "@id": "outbreaktest:Protocol"
      },
      "schema:rangeIncludes": [
        {
          "@id": "schema:Text"
        }
      ]
    },
    {
      "@id": "outbreaktest:correctionNote",
      "@type": "rdfs:Property",
      "rdfs:comment": "statement of what was updated/changed since prior version",
      "rdfs:label": "correctionNote",
      "schema:domainIncludes": {
        "@id": "outbreaktest:Protocol"
      },
      "schema:rangeIncludes": [
        {
          "@id": "schema:Text"
        }
      ]
    },
    {
      "@id": "outbreaktest:protocolStatus",
      "@type": "rdfs:Property",
      "rdfs:comment": "The status of the protocol",
      "rdfs:label": "protocolStatus",
      "schema:domainIncludes": {
        "@id": "outbreaktest:Protocol"
      },
      "schema:rangeIncludes": [
        {
          "@id": "schema:Text"
        }
      ]
    },
    {
      "@id": "outbreaktest:instrument",
      "@type": "rdfs:Property",
      "rdfs:comment": "A laboratory equipment use by a person to follow one or more steps described in this Protocol",
      "rdfs:label": "instrument",
      "schema:domainIncludes": {
        "@id": "outbreaktest:Protocol"
      },
      "schema:rangeIncludes": [
        {
          "@id": "schema:Product"
        }
      ]
    },
    {
      "@id": "outbreaktest:measurementTechnique",
      "@type": "rdfs:Property",
      "rdfs:comment": "A technique or technology used in this protocol",
      "rdfs:label": "measurementTechnique",
      "schema:domainIncludes": {
        "@id": "outbreaktest:Protocol"
      },
      "schema:rangeIncludes": [
        {
          "@id": "schema:Text"
        }
      ]
    },
    {
      "@id": "outbreaktest:protocolSetting",
      "@type": "rdfs:Property",
      "rdfs:comment": "The type of 'protocol' based on setting in which it would apply",
      "rdfs:label": "protocolSetting",
      "schema:domainIncludes": {
        "@id": "outbreaktest:Protocol"
      },
      "schema:rangeIncludes": [
        {
          "@id": "schema:Text"
        }
      ]
    },
    {
      "@id": "outbreaktest:protocolCategory",
      "@type": "rdfs:Property",
      "rdfs:comment": "The type of 'protocol' based on degree of specificity and purpose",
      "rdfs:label": "protocolCategory",
      "schema:domainIncludes": {
        "@id": "outbreaktest:Protocol"
      },
      "schema:rangeIncludes": [
        {
          "@id": "schema:Text"
        }
      ]
    },
    {
      "@id": "outbreaktest:usedToGenerate",
      "@type": "rdfs:Property",
      "rdfs:comment": "Type of evidence/data generated by the protocol",
      "rdfs:label": "usedToGenerate",
      "schema:domainIncludes": {
        "@id": "outbreaktest:Protocol"
      },
      "schema:rangeIncludes": [
        {
          "@id": "schema:Text"
        }
      ]
    },
    {
      "@id": "outbreaktest:inComplianceWith",
      "@type": "rdfs:Property",
      "rdfs:comment": "Guidelines or Standards to which these protocols adhere. ie- GMP, GCP, other EQUATOR guidelines. Useful for selecting protocols where certain standards/guidelines must be met",
      "rdfs:label": "inComplianceWith",
      "schema:domainIncludes": {
        "@id": "outbreaktest:Protocol"
      },
      "schema:rangeIncludes": [
        {
          "@id": "schema:Text"
        }
      ]
    }
  ]
}